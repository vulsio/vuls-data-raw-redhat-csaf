	"vulnerabilities": [
		{
			"acknowledgments": [
				{
					"names": [
						"Florian Weimer"
					],
					"organization": "Red Hat Product Security",
					"summary": "This issue was discovered by Red Hat."
				}
			],
			"cve": "CVE-2013-2099",
			"cwe": {
				"id": "CWE-407",
				"name": "Inefficient Algorithmic Complexity"
			},
			"discovery_date": "2013-05-15T00:00:00+00:00",
			"ids": [
				{
					"system_name": "Red Hat Bugzilla ID",
					"text": "963260"
				}
			],
			"notes": [
				{
					"category": "description",
					"text": "A denial of service flaw was found in the way Python's SSL module implementation performed matching of certain certificate names. A remote attacker able to obtain a valid certificate that contained multiple wildcard characters could use this flaw to issue a request to validate such a certificate, resulting in excessive consumption of CPU.",
					"title": "Vulnerability description"
				},
				{
					"category": "summary",
					"text": "python: ssl.match_hostname() DoS via certificates with specially crafted hostname wildcard patterns",
					"title": "Vulnerability summary"
				},
				{
					"category": "other",
					"text": "This issue did not affect the versions of python as shipped with Red Hat Enterprise Linux 5 and 6 as the SSL module there did not implement the match_hostname() routine yet.",
					"title": "Statement"
				},
				{
					"category": "general",
					"text": "The CVSS score(s) listed for this vulnerability do not reflect the associated product's status, and are included for informational purposes to better understand the severity of this vulnerability.",
					"title": "CVSS score applicability"
				}
			],
			"product_status": {
