	"vulnerabilities": [
		{
			"cve": "CVE-2019-16167",
			"cwe": {
				"id": "CWE-190",
				"name": "Integer Overflow or Wraparound"
			},
			"discovery_date": "2019-11-05T00:00:00+00:00",
			"ids": [
				{
					"system_name": "Red Hat Bugzilla ID",
					"text": "1768970"
				}
			],
			"notes": [
				{
					"category": "description",
					"text": "An integer overflow vulnerability was found in sysstat in the way the `sadf` command processes the contents of data files created by the `sar` command. A local attacker could exploit this flaw by creating a specially crafted file with malformed data that, when loaded by a victim, causes the application to crash.",
					"title": "Vulnerability description"
				},
				{
					"category": "summary",
					"text": "sysstat: memory corruption due to an integer overflow in remap_struct in sa_common.c",
					"title": "Vulnerability summary"
				},
				{
					"category": "other",
					"text": "This issue did not affect the versions of sysstat as shipped with Red Hat Enterprise Linux 5, 6, and 7 as they did not include the vulnerable function, which was introduced in a newer version of the package.",
					"title": "Statement"
				},
				{
					"category": "general",
					"text": "The CVSS score(s) listed for this vulnerability do not reflect the associated product's status, and are included for informational purposes to better understand the severity of this vulnerability.",
					"title": "CVSS score applicability"
				}
			],
