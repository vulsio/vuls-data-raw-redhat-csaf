	"vulnerabilities": [
		{
			"acknowledgments": [
				{
					"names": [
						"Richard W. M. Jones"
					],
					"organization": "Red Hat",
					"summary": "This issue was discovered by Red Hat."
				}
			],
			"cve": "CVE-2019-14850",
			"cwe": {
				"id": "CWE-406",
				"name": "Insufficient Control of Network Message Volume (Network Amplification)"
			},
			"discovery_date": "2019-09-11T00:00:00+00:00",
			"ids": [
				{
					"system_name": "Red Hat Bugzilla ID",
					"text": "1757258"
				}
			],
			"notes": [
				{
					"category": "description",
					"text": "A denial of service vulnerability was discovered in nbdkit. An attacker could connect to the nbdkit service and cause it to perform a large amount of work in initializing backend plugins, by simply opening a connection to the service. This vulnerability could cause resource consumption and degradation of service in nbdkit, depending on the plugins configured on the server-side.",
					"title": "Vulnerability description"
				},
				{
					"category": "summary",
					"text": "nbdkit: denial of service due to premature opening of back-end connection",
					"title": "Vulnerability summary"
				},
				{
					"category": "other",
					"text": "Red Hat Virtualization Host includes nbdkit as a dependency of libguestfs, and uses it in p2v/v2v migrations. libguestfs uses nbdkit with a Unix domain socket only, so this vulnerability is not exposed to untrusted users.\n\nRed Hat Virtualization 4.2 EUS includes a vulnerable version of nbdkit, however it is used only on the Hypervisor with a Unix domain socket, so this vulnerability is not exposed to untrusted users.",
					"title": "Statement"
				},
				{
					"category": "general",
					"text": "The CVSS score(s) listed for this vulnerability do not reflect the associated product's status, and are included for informational purposes to better understand the severity of this vulnerability.",
					"title": "CVSS score applicability"
				}
			],
			"product_status": {
				"fixed": [
					"7Server-7.8:nbdkit-0:1.8.0-3.el7.src",
					"7Server-7.8:nbdkit-0:1.8.0-3.el7.x86_64",
					"7Server-7.8:nbdkit-basic-plugins-0:1.8.0-3.el7.x86_64",
					"7Server-7.8:nbdkit-debuginfo-0:1.8.0-3.el7.x86_64",
