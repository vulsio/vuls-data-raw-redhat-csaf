			"notes": [
				{
					"category": "description",
					"text": "A business logic flaw was found in Red Hat CloudForms where the read-only values of the Widgets could be altered. An attacker with low privileges could bypass server-side validation by dropping the disabled attribute from the fields.",
					"title": "Vulnerability description"
				},
				{
					"category": "summary",
					"text": "CVE-2020-10778 CloudForms: Business logic bypass through widgets",
					"title": "Vulnerability summary"
				},
				{
					"category": "general",
					"text": "The CVSS score(s) listed for this vulnerability do not reflect the associated product's status, and are included for informational purposes to better understand the severity of this vulnerability.",
					"title": "CVSS score applicability"
				}
			],
			"product_status": {
