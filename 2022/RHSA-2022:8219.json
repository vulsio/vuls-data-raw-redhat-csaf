	"vulnerabilities": [
		{
			"cve": "CVE-2022-1328",
			"cwe": {
				"id": "CWE-120",
				"name": "Buffer Copy without Checking Size of Input ('Classic Buffer Overflow')"
			},
			"discovery_date": "2022-04-17T00:00:00+00:00",
			"ids": [
				{
					"system_name": "Red Hat Bugzilla ID",
					"text": "2076058"
				}
			],
			"notes": [
				{
					"category": "description",
					"text": "A flaw was found in mutt. When reading unencoded messages, mutt uses the line length from the untrusted input without any validation. This flaw allows an attacker to craft a malicious message, which leads to an out-of-bounds read, causing data leaks that include fragments of other unrelated messages.",
					"title": "Vulnerability description"
				},
				{
					"category": "summary",
					"text": "mutt: buffer overflow in uudecoder function",
					"title": "Vulnerability summary"
				},
				{
					"category": "general",
					"text": "The CVSS score(s) listed for this vulnerability do not reflect the associated product's status, and are included for informational purposes to better understand the severity of this vulnerability.",
					"title": "CVSS score applicability"
				}
			],
