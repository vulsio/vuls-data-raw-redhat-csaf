	"vulnerabilities": [
		{
			"cve": "CVE-2013-4854",
			"discovery_date": "2013-07-26T00:00:00+00:00",
			"ids": [
				{
					"system_name": "Red Hat Bugzilla ID",
					"text": "988999"
				}
			],
			"notes": [
				{
					"category": "description",
					"text": "The RFC 5011 implementation in rdata.c in ISC BIND 9.7.x and 9.8.x before 9.8.5-P2, 9.8.6b1, 9.9.x before 9.9.3-P2, and 9.9.4b1, and DNSco BIND 9.9.3-S1 before 9.9.3-S1-P1 and 9.9.4-S1b1, allows remote attackers to cause a denial of service (assertion failure and named daemon exit) via a query with a malformed RDATA section that is not properly handled during construction of a log message, as exploited in the wild in July 2013.",
					"title": "Vulnerability description"
				},
				{
					"category": "summary",
					"text": "bind: named crash with an assertion failure on parsing malformed rdata",
					"title": "Vulnerability summary"
				},
				{
					"category": "other",
					"text": "This issue did not affect the versions of bind as shipped with Red Hat Enterprise Linux 5.  It does affect the versions of bind97 as shipped with Red Hat Enterprise Linux 5 and the versions of bind as shipped with Red Hat Enterprise Linux 6.",
					"title": "Statement"
				},
				{
					"category": "general",
					"text": "The CVSS score(s) listed for this vulnerability do not reflect the associated product's status, and are included for informational purposes to better understand the severity of this vulnerability.",
					"title": "CVSS score applicability"
				}
			],
