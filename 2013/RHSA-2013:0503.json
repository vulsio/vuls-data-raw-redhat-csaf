			"notes": [
				{
					"category": "description",
					"text": "389 Directory Server 1.2.10 does not properly update the ACL when a DN entry is moved by a modrdn operation, which allows remote authenticated users with certain permissions to bypass ACL restrictions and access the DN entry.",
					"title": "Vulnerability description"
				},
				{
					"category": "summary",
					"text": "Change on SLAPI_MODRDN_NEWSUPERIOR is not evaluated in ACL (ACL rules bypass possible)",
					"title": "Vulnerability summary"
				},
