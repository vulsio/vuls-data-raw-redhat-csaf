	"vulnerabilities": [
		{
			"cve": "CVE-2023-43622",
			"cwe": {
				"id": "CWE-400",
				"name": "Uncontrolled Resource Consumption"
			},
			"discovery_date": "2023-10-19T00:00:00+00:00",
			"ids": [
				{
					"system_name": "Red Hat Bugzilla ID",
					"text": "2245153"
				}
			],
			"notes": [
				{
					"category": "description",
					"text": "A flaw was found in the mod_http2 module of httpd. This flaw allows an attacker opening an HTTP/2 connection with an initial window size of 0 to block handling of that connection indefinitely. This vulnerability can exhaust worker resources in the server, similar to the well-known \"slow loris\" attack pattern.",
					"title": "Vulnerability description"
				},
				{
					"category": "summary",
					"text": "httpd: mod_http2: DoS in HTTP/2 with initial window size 0",
					"title": "Vulnerability summary"
				},
				{
					"category": "other",
					"text": "This flaw only affects configurations with mod_http2 loaded and being used. Also, if there is no HTTP2 server configured, the httpd server is not affected and no further mitigation is needed.\n\nThe httpd mod_http2 module is enabled by default on Red Hat Enterprise Linux 8 and 9 via the mod_http2 package. However, there is no HTTP2 server configured by default.",
					"title": "Statement"
				},
				{
					"category": "general",
					"text": "The CVSS score(s) listed for this vulnerability do not reflect the associated product's status, and are included for informational purposes to better understand the severity of this vulnerability.",
					"title": "CVSS score applicability"
				}
			],
			"product_status": {
				"fixed": [
					"AppStream-9.4.0.GA:mod_http2-0:2.0.26-1.el9.aarch64",
