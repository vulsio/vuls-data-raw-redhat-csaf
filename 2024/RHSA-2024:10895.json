		"notes": [
			{
				"category": "summary",
				"text": "Cost Management Metrics Operator version 3.3.2 release.",
				"title": "Topic"
			},
			{
				"category": "general",
				"text": "The Cost Management Metrics Operator is a component of the Red Hat Cost Managment service for Openshift. The operator runs on the latest supported versions of Openshift. This operator obtains OpenShift usage data by querying Prometheus every hour to create metric reports that it uploads to Cost Management at console.redhat.com.",
				"title": "Details"
			},
