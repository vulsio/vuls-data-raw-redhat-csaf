	"vulnerabilities": [
		{
			"cve": "CVE-2018-10841",
			"cwe": {
				"id": "CWE-287",
				"name": "Improper Authentication"
			},
			"discovery_date": "2018-05-15T00:00:00+00:00",
			"ids": [
				{
					"system_name": "Red Hat Bugzilla ID",
					"text": "1582043"
				}
			],
			"notes": [
				{
					"category": "description",
					"text": "A flaw was found in glusterfs which can lead to privilege escalation on gluster server nodes. An authenticated gluster client via TLS could use gluster cli with --remote-host command to add it self to trusted storage pool and perform privileged gluster operations like adding other machines to trusted storage pool, start, stop, and delete volumes.",
					"title": "Vulnerability description"
				},
				{
					"category": "summary",
					"text": "glusterfs: access trusted peer group via remote-host command",
					"title": "Vulnerability summary"
				},
				{
					"category": "other",
					"text": "Red Hat Enterprise Linux 6, 7 are not affected by this flaw as it only affects glusterfs-server package. Red Hat Virtualization Hypervisor is not impacted by this flaw, as it uses gluster in a controlled manner via vdsm.",
					"title": "Statement"
				},
				{
					"category": "general",
					"text": "The CVSS score(s) listed for this vulnerability do not reflect the associated product's status, and are included for informational purposes to better understand the severity of this vulnerability.",
					"title": "CVSS score applicability"
				}
			],
